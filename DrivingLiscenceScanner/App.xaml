<Application x:Class="DrivingLiscenceScanner.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/ViewModelLocator.xaml" />
                <ResourceDictionary Source="Resources/BaseStyles.xaml" />
                <ResourceDictionary Source="Resources/TitleBarStyles.xaml" />
                <ResourceDictionary Source="Resources/ViewDataTemplates.xaml" />
                <ResourceDictionary Source="Resources/ScanViewStyles.xaml" />
                <ResourceDictionary Source="Resources/MainWindowStyles.xaml" />
                <ResourceDictionary Source="Resources/ScannedViewStyles.xaml" />
                <ResourceDictionary Source="CustomersViewStyles.xaml" />
                <ResourceDictionary Source="SettingsViewStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>